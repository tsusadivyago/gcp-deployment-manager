info:
  title: Boomi GKE Molecule Bastion Machine
  author: Vilvamani Arumugam <vilvamani007@gmail.com>
  description: Deploy Dell Boomi Molecule in Bastion Machine
  version: 1.0

imports:
- path: bastion.jinja

required:
  - region
  - zone
  - machineType

properties:
  region:
    type: string
    description: "Google Cloud Region to deploy Boomi Molecule stack"
    default: us-central1


