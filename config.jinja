resources:
- name: boomi-gke-network
  type: network.jinja
  properties:
    region: {{ properties["region"] }}

- name: boomi-gke-bastion
  type: bastion.jinja
  properties:
    region: {{ properties["region"] }}
    zone: {{ properties["zone"] }}
  metadata:
    dependsOn:
      - boomi-gke-network